{
  "name": "meius/laravel-filter",
  "description": "A Laravel package for applying dynamic filters to Eloquent models based on request parameters.",
  "keywords": [
    "laravel",
    "filter",
    "eloquent",
    "dynamic",
    "request",
    "model",
    "attributes"
  ],
  "type": "library",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Meius\\LaravelFilter\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Meius\\LaravelFilter\\Tests\\": "tests/"
    }
  },
  "authors": [
    {
      "name": "Bohdan Meius",
      "email": "bohdan.meius@gmail.com"
    }
  ],
  "minimum-stability": "dev",
  "require": {
    "php": "^8",
    "illuminate/console": "^8 || ^9 || ^10 || ^11",
    "illuminate/database": "^8 || ^9 || ^10 || ^11",
    "illuminate/filesystem": "^8 || ^9 || ^10 || ^11",
    "illuminate/http": "^8 || ^9 || ^10 || ^11",
    "illuminate/log": "^8 || ^9 || ^10 || ^11",
    "illuminate/routing": "^8 || ^9 || ^10 || ^11",
    "illuminate/support": "^8 || ^9 || ^10 || ^11"
  },
  "require-dev": {
    "phpstan/phpstan": "^2.0",
    "friendsofphp/php-cs-fixer": "*",
    "phpunit/phpunit": "^9.6 || ^10.0",
    "orchestra/testbench": "^6 || ^7 || ^8 || ^9"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Meius\\LaravelFilter\\Providers\\AppServiceProvider",
        "Meius\\LaravelFilter\\Providers\\FilterServiceProvider",
        "Meius\\LaravelFilter\\Providers\\LoggerServiceProvider"
      ]
    }
  }
}
